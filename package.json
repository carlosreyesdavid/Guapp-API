  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carlosreyesdavid/Guapp-API/issues"
  },
  "homepage": "https://github.com/carlosreyesdavid/Guapp-API#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "mongoose": "^4.6.3",
    "simple-logger": "^1.0.1",
    "simple-node-logger": "^0.93.12",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "request": "^2.75.0",
    "supertest": "^2.0.0"
